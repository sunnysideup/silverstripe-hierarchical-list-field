{
    "name": "sunnysideup/hierarchical-list-field",
    "description": "A hierarchical-list-field for Silverstripe CMS. This module allows you to create a read-only list form field with items that can be expanded and collapsed.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "hierarchical",
        "list",
        "expandable",
        "accordion",
        "list",
        "content browser"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\HierarchicalListField\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/sake-npm-build-hierarchical-list-field.sh",
        "bin/sake-npm-watch-hierarchical-list-field.sh"
    ],
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}
